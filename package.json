{
  "private": true,
  "type": "module",
  "packageManager": "bun@1.2.21",
  "engines": {
    "node": "23.9.0",
    "bun": "1.2.21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Guiguerreiro39/effect-monorepo.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "node scripts/clean.mjs",
    "codegen": "bun --recursive --parallel run codegen",
    "build": "tsc -b tsconfig.build.json && bun --recursive --parallel run build",
    "check": "tsc -b tsconfig.json",
    "check:recursive": "bun --recursive exec tsc -b tsconfig.json",
    "check:all": "bun lint && bun check && bun test",
    "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs,tsx}\" --quiet",
    "lint:fix": "bun lint --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"**/*.{js,ts,mjs,jsx,tsx,json,md,css,html,yaml,yml,mdx}\"",
    "format:check": "prettier --check \"**/*.{js,ts,mjs,jsx,tsx,json,md,css,html,yaml,yml,mdx}\"",
    "coverage": "vitest --coverage",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@effect/build-utils": "0.7.9",
    "@effect/eslint-plugin": "0.3.0",
    "@effect/language-service": "0.9.1",
    "@effect/vitest": "0.20.17",
    "@eslint/compat": "1.2.2",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.13.0",
    "@types/node": "^22.18.0",
    "@typescript-eslint/eslint-plugin": "8.14.0",
    "@typescript-eslint/parser": "8.14.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "effect": "3.14.16",
    "eslint": "9.13.0",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-codegen": "^0.29.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "3.4.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^2.1.9",
    "read-package-up": "^11.0.0"
  }
}
